{
  "name": "probot-serverless-openwhisk",
  "version": "0.13.3",
  "description": "Probot Serverless OpenWisk",
  "license": "Apache-2.0",
  "repository": "https://github.com/tripodsan/probot-serverless-openwhisk",
  "bugs": "https://github.com/tripodsan/probot-serverless-openwhisk/issues",
  "keywords": [
    "probot",
    "github",
    "openwhisk"
  ],
  "bin": {
    "wskbot": "src/cli/index.js"
  },
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "test": "nyc mocha && npm run lint",
    "test-ci": "npm run lint && nyc mocha --reporter mocha-junit-reporter && nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "preversion": "npm test",
    "postversion": "git push origin master --follow-tags",
    "delete-git-tag": "git tag -d v$npm_package_version && git push origin :v$npm_package_version"
  },
  "dependencies": {
    "@adobe/openwhisk-action-builder": "0.2.2",
    "@tripod/bunyan-syslog": "^0.3.2",
    "bunyan": "^1.8.12",
    "bunyan-loggly": "^1.3.5",
    "chalk": "^2.4.1",
    "dotenv": "^6.0.0",
    "fs-extra": "^7.0.0",
    "openwhisk": "^3.17.0",
    "probot": "7.5.1"
  },
  "devDependencies": {
    "ajv": "^6.9.1",
    "codecov": "^3.2.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-header": "^2.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.11.0",
    "mocha": "^6.0.1",
    "mocha-junit-reporter": "^1.18.0",
    "nyc": "^13.3.0",
    "yargs": "^13.2.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  }
}
